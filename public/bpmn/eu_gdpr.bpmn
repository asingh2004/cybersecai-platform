<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:process id="GDPRBreachProcess">
    <bpmn:startEvent id="StartEvent" name="Breach Detected"/>
    <bpmn:task id="T1" name="Assess Risk & Impact"/>
    <bpmn:task id="T2" name="Notify DPO"/>
    <bpmn:exclusiveGateway id="G1" name="Is Notification Required?"/>
    <bpmn:task id="T3" name="Notify Regulator/Affected"/>
    <bpmn:task id="T4" name="Document Incident & Remediation"/>
    <bpmn:endEvent id="EndEvent" name="Process Closed"/>
    <bpmn:sequenceFlow id="f1" sourceRef="StartEvent" targetRef="T1"/>
    <bpmn:sequenceFlow id="f2" sourceRef="T1" targetRef="T2"/>
    <bpmn:sequenceFlow id="f3" sourceRef="T2" targetRef="G1"/>
    <bpmn:sequenceFlow id="f4" sourceRef="G1" targetRef="T3">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${notificationRequired}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="f5" sourceRef="T3" targetRef="T4"/>
    <bpmn:sequenceFlow id="f6" sourceRef="G1" targetRef="T4">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${!notificationRequired}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="f7" sourceRef="T4" targetRef="EndEvent"/>
  </bpmn:process>
</bpmn:definitions>